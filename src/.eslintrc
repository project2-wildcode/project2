{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [0],
    "no-tabs": [0],
    "no-mixed-spaces-and-tabs": [0],
    "linebreak-style": [0],
    "quotes": [0],
    "jsx-a11y/click-events-have-key-events": [0],
    "comma-dangle": [0]
  }
}
